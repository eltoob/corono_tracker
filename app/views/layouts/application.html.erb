<!DOCTYPE html>
<html>
  <head>
    <title> ncov19live.com</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    <link href="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.8/js/select2.min.js" defer></script>

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta charset="utf-8" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="https://coderthemes.com/codefox/layouts/vertical/assets/images/favicon.ico">

    <!-- Bootstrap select pluings -->
    <link href="https://coderthemes.com/codefox/layouts/vertical/assets/libs/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" type="text/css" />

    <!-- c3 plugin css -->
    <link rel="stylesheet" type="text/css" href="https://coderthemes.com/codefox/layouts/vertical/assets/libs/c3/c3.min.css">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-160510419-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
    
      gtag('config', 'UA-160510419-1');
    </script>
    

  </head>

  <body>
    <%= yield %>
  </body>
</html>

